public class FibonacciSeries {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        {
        int n1=0,n2=1,n3,i,count=10;
        System.out.println(n1+""+n2);
        
        for(i=2;i<count;++i)
        {
            n3=n1+n2;
            System.out.print(""+n3);
            n1=n2;
            n2=n3;
            
        }
